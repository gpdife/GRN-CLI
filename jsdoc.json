{
  "tags": {
      "allowUnknownTags": false
  },
  "source": {
      "include": "../GRN-CLI",
      "includePattern": ".js$",
      "excludePattern":   "(node_modules/|docs)",
      "exclude": [ "node_modules" ]
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts": {
      "template": "node_modules/docdash",
      "encoding": "utf8",
      "destination": "./docs/",
      "recurse": true,
      "verbose": true
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  }
}
